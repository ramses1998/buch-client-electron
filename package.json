    "@mui/joy": "^5.0.0-beta.27",
    "axios": "^1.6.7",
    "electron-serve": "^1.3.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^6.1.8",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/axios": "^0.14.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
