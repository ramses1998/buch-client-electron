{
  "name": "buch-client-electron",
  "version": "0.1.0",
  "description": "Desktop-Anwendung mit Electron, NEXT.js und Material UI",
  "author": "Ramses II Tetang Zebowve",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "next:dev": "next dev --port 3001 --experimental-https --experimental-https-cert src/tls/certificate.crt --experimental-https-key src/tls/key.pem",
    "electron:dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"npm run next:dev\" \"electron .\"",
    "next:start": "serve out -p 3001 --cors --ssl-cert src/tls/certificate.crt --ssl-key src/tls/key.pem",
    "build": "next build && electron-builder --windows --x64",
    "next:build": "next build",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.16",
    "@mui/icons-material": "^5.15.9",
    "@mui/joy": "^5.0.0-beta.27",
    "axios": "^1.6.7",
    "electron-serve": "^1.3.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.12.2",
    "serve": "^14.2.1",
    "styled-components": "^6.1.8",
    "swr": "^2.2.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/axios": "^0.14.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "concurrently": "^8.2.2",
    "electron": "^29.1.4",
    "electron-builder": "^24.9.1",
    "eslint": "^8",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-regexp": "^2.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5"
  }
}
